&define ABS(p1) (IIF(p1<0,-p1,p1))
&define MIN(p1,p2) (IIF(p1<p2,p1,p2))
&define MAX(p1,p2) (IIF(p1>p2,p1,p2))
&define AND(p1,p2) (IIF(p1 AND p2, TRUE, FALSE))
&define OR(p1,p2) (IIF(p1 OR p2, TRUE, FALSE))
&define XOR(p1,p2) (IIF(p1 AND NOT p2, TRUE, IIF(NOT p1 AND p2, TRUE, FALSE)))
&define DMY(p1,p2,p3) MDY(p2,p1,p3)
&define YMD(p1,p2,p3) MDY(p2,p3,p1)